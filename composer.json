{
  "name": "w3scout/contao-dompdf-bundle",
  "homepage":"https://github.com/w3scout/contao-dompdf-bundle",
  "description": "Use DOMPDF to export articles as PDF.",
  "keywords":["contao","dompdf","pdf"],
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Darko Selesi",
      "homepage": "https://w3scouts.com",
      "role":"Developer"
    }
  ],
  "support":{
      "email":"hallo@w3scouts.com",
      "issues":"https://github.com/w3scout/contao-dompdf-bundle/issues",
      "source":"https://github.com/w3scout/contao-dompdf-bundle"
  },
  "require": {
    "php": ">=7.1.0",
    "contao/core-bundle": "^4.4 || ^5.0" ,
    "dompdf/dompdf": ">=2.0.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "W3Scout\\DompdfBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "W3Scout\\DompdfBundle\\ContaoManager\\Plugin"
  }
}
